{
  "mcpServers": {
    "file-server": {
      "command": "mcp-file-server",
      "env": {
        "PYTHONPATH": "/path/to/your/mcp-file-server/src"
      },
      "disabled": false,
      "autoApprove": []
    },
    "file-server-uv": {
      "command": "uv",
